{
  "name": "skydb_app",
  "version": "0.1.0",
  "private": true,
  "keywords": [
    "blockchain",
    "ethereum",
    "erc721",
    "react",
    "redux",
    "javascript",
    "solidity"
  ],
  "author": "Vault Labs",
  "scripts": {
    "coverage": "yarn run test --coverage",
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "format:jsx": "prettier --write 'src/**/*.{js,jsx,scss,less}' && yarn lint:eslint --fix",
    "format:prettier": "prettier --write 'src/**/*.{js,jsx,scss,less}'",
    "format": "yarn run format:jsx && yarn run format:css",
    "format:check": "prettier -c 'src/**/*.{js,jsx,scss,less}'",
    "format:css": "stylelint --fix --syntax scss \"src/**/*.scss\"",
    "lint": "yarn lint:eslint && yarn lint:css",
    "lint:css": "stylelint \"src/**/*.{scss,less}\"",
    "lint:css:fix": "stylelint \"src/**/*.{scss,less}\" --fix",
    "lint:eslint": "eslint 'src/**/*.{js,jsx}'"
  },
  "dependencies": {
    "3id-connect": "^0.1.0",
    "@ant-design/icons": "^4.2.2",
    "@apollo/react-hooks": "^4.0.0",
    "@astraldao/astral-protocol-contracts": "^0.0.32",
    "@astraldao/astral-protocol-core": "^0.0.34",
    "@astraldao/stac-validator-js": "^0.0.20",
    "@ceramicnetwork/3id-did-resolver": "^0.4.9",
    "@ceramicnetwork/ceramic-http-client": "^0.7.3",
    "@ceramicnetwork/key-did-resolver": "^0.1.3",
    "@ceramicstudio/idx-tools": "^0.3.1",
    "@ceramicstudio/idx-web": "^0.2.0",
    "@loadable/component": "^5.13.2",
    "@mapbox/geojsonhint": "^3.0.0",
    "@portis/web3": "^2.0.0-beta.59",
    "@toruslabs/torus-embed": "^1.8.4",
    "@urbica/react-map-gl": "^1.14.2",
    "@walletconnect/web3-provider": "^1.2.2",
    "antd": "^4.6.4",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link": "^1.2.14",
    "apollo-link-http": "^1.5.17",
    "apollo-link-ws": "^1.0.20",
    "apollo-utilities": "^1.3.4",
    "axios": "^0.20.0",
    "babel-plugin-import": "^1.13.0",
    "bootstrap-css-only": "^4.4.1",
    "classnames": "^2.2.6",
    "connected-react-router": "^6.8.0",
    "d3-ease": "^2.0.0",
    "ethereum-blockies-base64": "^1.0.2",
    "fortmatic": "^2.2.1",
    "graphql": "^15.3.0",
    "graphql-tag": "^2.11.0",
    "history": "4.10.1",
    "lodash": "^4.17.20",
    "mapbox-gl": "^1.12.0",
    "memoize-one": "^5.1.1",
    "moment": "^2.29.0",
    "qs": "^6.9.4",
    "react": "^16.13.1",
    "react-code-blocks": "^0.0.8",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.21",
    "react-map-gl": "^5.2.8",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "react-scrollbars-custom": "^4.0.25",
    "react-virtualized-auto-sizer": "^1.0.2",
    "react-window": "^1.8.5",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.1.3",
    "skynet-js": "^2.9.0",
    "squarelink": "^1.1.4",
    "store": "^2.0.12",
    "subscriptions-transport-ws": "^0.9.18",
    "uint8arrays": "^1.1.0",
    "web3": "^1.3.0",
    "web3modal": "^1.9.0"
  },
  "devDependencies": {
    "@hot-loader/react-dom": "^16.13.0",
    "babel-eslint": "^10.1.0",
    "customize-cra": "^1.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.4",
    "enzyme-to-json": "^3.5.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "husky": "^4.3.0",
    "less": "^3.12.2",
    "less-loader": "^7.0.1",
    "lint-staged": "^10.3.0",
    "node-sass": "^4.14.1",
    "prettier": "^2.1.2",
    "react-app-rewired": "^2.1.6",
    "react-test-renderer": "^16.13.1",
    "stylelint": "^13.7.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-prettier": "^1.1.2",
    "stylelint-scss": "^3.18.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,scss,less}": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.{js,jsx}": [
      "eslint"
    ],
    "src/**/*.{scss,less}": [
      "stylelint"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.jsx",
      "!src/setupTests.js",
      "!src/components/**/index.{js,jsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
